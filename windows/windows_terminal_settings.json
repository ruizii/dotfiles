{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions":
    [
        {
            "command":
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+shift+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+shift+v"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command":
            {
                "action": "sendInput",
                "input": "\u001b[27;5;9~"
            },
            "keys": "ctrl+tab"
        },
        {
            "command":
            {
                "action": "nextTab"
            },
            "keys": "ctrl+shift+right"
        },
        {
            "command":
            {
                "action": "sendInput",
                "input": "\u001b[27;6;9~"
            },
            "keys": "ctrl+shift+tab"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+shift+enter"
        },
        {
            "command":
            {
                "action": "prevTab"
            },
            "keys": "ctrl+shift+left"
        },
        {
            "command": "toggleFocusMode",
            "keys": "f12"
        }
    ],
    "alwaysShowTabs": true,
    "centerOnLaunch": false,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{46ca431a-3a87-5fb3-83cd-11ececc031d2}",
    "initialCols": 131,
